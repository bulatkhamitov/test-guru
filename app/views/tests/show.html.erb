<p>
  <strong>Title:</strong>
  <%= @test.title %>
</p>

<p>
  <strong>Level:</strong>
  <%= @test.level %>
</p>

<table border=1 frame=hsides rules=rows,columns>
  <tr>
    <th>Question</th>
    <th colspan="3">Actions</th>
  </tr>
  <%= render @test.questions %>
</table>

<%= link_to 'Create new question', new_test_question_path(@test) %> |
<%= link_to 'Edit test', edit_test_path(@test) %> |
<%= link_to 'Back', tests_path %>
