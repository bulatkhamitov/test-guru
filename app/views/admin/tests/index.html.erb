<h1>List of tests | <%= link_to 'Create new', new_admin_test_path %></h1>
<%= render 'shared/errors', resource: @test %>
<table class="table", border=1, frame=hsides,vsides, rules=rows,columns, style="width:100%">
  <tr>
    <th class="sort-by-title pointer">
      Title
      <%= octicon 'arrow-up', class: 'text-success hide' %>
      <%= octicon 'arrow-down', class: 'text-success hide' %>
    </th>
    <th>Level</th>
    <th>Questions</th>
    <th colspan="3">Actions</th>
  </tr>

  <%= render @tests %>
</table>
